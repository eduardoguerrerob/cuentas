<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Panel>
        <Table items="jsonSolicitudeModel>/solicitudes/cuentas">
            <columns>
                <Column >
                    <header>
                        <Label text="{i18n>requestNumber}"/>
                    </header>
                </Column>
                 <Column >
                    <header>
                        <Label text="{i18n>requestName}"/>
                    </header>
                </Column>
            </columns>
            <items>
                <ColumnListItem >
                    <cells>
                        <Label text="{jsonSolicitudeModel>numero}"/>
                        <Label text="{jsonSolicitudeModel>nombre}"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Panel>
</core:FragmentDefinition>